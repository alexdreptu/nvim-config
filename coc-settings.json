{
    "coc.preferences.extensionUpdateCheck": "daily",
    "coc.source.around.firstMatch": false,
    "coc.source.buffer.firstMatch": false,
    "diagnostic.enableMessage": "jump",
    "diagnostic.messageTarget": "float",
    "signature.enable": true,
    "signature.target": "float",
    "pyright.enable": true,
    "pyright.organizeimports.provider": "pyright",
    "pyright.completion.snippetSupport": true,
    "pyright.disableDiagnostics": false,
    "python.analysis.diagnosticMode": "openFilesOnly",
    "suggest.triggerCompletionWait": 30,
    "python.formatting.provider": "none",
    "python.linting.enabled": false,
    "go.enable": true,
    "typescript.format.enabled": false,
    "typescript.referencesCodeLens.enable": false,
    "typescript.preferences.quoteStyle": "single",
    "yaml.format.enable": true,
    "jedi.enable": false,
    "jedi.jediSettings.caseInsensitiveCompletion": true,
    "jedi.completion.resolveEagerly": false
}
